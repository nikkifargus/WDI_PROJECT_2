<h1><%= user.username %>'s Profile Page</h1>

<h3>All My Recipes</h3>

<div class="row ">
  <% recipes.forEach(recipe => { %>
    <div class="col s12 m4 l3">
      <div class="indexPhotoFrame z-depth-3">
        <div class="indexPhoto">
          <a href="/recipes/<%= recipe._id%>">
          <img src="<%= recipe.image %>">
        </div>
        <div class="indexInfo">
          <p class="black-text"> <%= recipe.name %><p>
          <p class="black-text">Created By Me!</p>
        </div>
      </div>
    </div>
  <%}); %>
</div>
