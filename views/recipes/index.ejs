<h1>All recipes</h1>

<div class="row ">
  <% recipes.forEach(recipe => { %>
    <div class="col s12 m4 l3">
      <div class="indexPhotoFrame z-depth-3">
        <div class="indexPhoto">
          <a href="/recipes/<%= recipe._id%>">
          <img src="<%= recipe.image %>">
        </div>
        <div class="indexInfo">
          <p> <%= recipe.name %><p>
          <p>By <%= recipe.createdBy.username %></p>
        </div>
      </div>
    </div>
  <%}); %>
</div>
